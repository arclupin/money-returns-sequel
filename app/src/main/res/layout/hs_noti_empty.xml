<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="wrap_content"
    android:id="@+id/hs_noti_row_container"
    android:background="@null"
    android:clickable="false"
   >
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent" android:layout_height="match_parent"
        android:orientation="vertical"
        android:id="@+id/house_result_container"
        android:clickable="true"
        android:background="@drawable/button_light_color"
        android:onClick="showJoinDialog"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        >

    <TextView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:id="@+id/house_result_description"
        android:paddingLeft="10dp"
        android:text="@string/noti_empty"
        android:paddingBottom="10dp"
        style="@style/HS_Search_Empty"
        android:layout_weight="1"
        android:gravity="center"
        />
    </LinearLayout>

</TableRow>
